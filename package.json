{
  "name": "sp-download",
  "description": "Easy to use files download client library and CLI for SharePoint in Node.js",
  "version": "1.0.6",
  "main": "./dist/index.js",
  "typings": "./dist/index",
  "scripts": {
    "build": "npm run tslint && npm run clean && npm run tsc",
    "tslint": "tslint -p .",
    "tsc": "tsc -p .",
    "clean": "rimraf ./dist",
    "test": "ts-node ./test/init && mocha --opts test/mocha.opts || ECHO.",
    "test:ci": "mocha --opts test/mocha.opts --ci || ECHO."
  },
  "bin": {
    "sp-download": "./dist/cli.js"
  },
  "keywords": [
    "sharepoint",
    "download",
    "client",
    "nodejs",
    "CLI"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koltyakov/sp-download.git"
  },
  "author": "Andrew Koltyakov <andrew.koltyakov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koltyakov/sp-download/issues"
  },
  "homepage": "https://github.com/koltyakov/sp-download#readme",
  "dependencies": {
    "colors": "^1.3.3",
    "commander": "^2.20.0",
    "node-sp-auth-config": "^2.6.4",
    "request": "^2.88.0",
    "sp-request": "^2.1.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/colors": "^1.2.1",
    "@types/commander": "^2.12.2",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.2",
    "@types/request": "^2.48.1",
    "chai": "^4.2.0",
    "cpass": "^2.1.0",
    "dotenv": "^8.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^6.1.4",
    "rimraf": "^2.6.3",
    "spsave": "^3.1.5",
    "ts-node": "^8.1.1",
    "tslint": "^5.16.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.5"
  }
}
