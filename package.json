{
  "name": "sp-download",
  "description": "Easy to use files download client library and CLI for SharePoint in Node.js",
  "version": "1.1.0",
  "main": "./dist/index.js",
  "typings": "./dist/index",
  "scripts": {
    "build": "npm run tslint && npm run clean && npm run tsc",
    "tslint": "tslint -p .",
    "tsc": "tsc -p .",
    "clean": "rimraf ./dist",
    "test": "ts-node ./test/init && mocha --opts test/mocha.opts || ECHO.",
    "test:ci": "mocha --opts test/mocha.opts --ci || ECHO."
  },
  "bin": {
    "sp-download": "./dist/cli.js"
  },
  "keywords": [
    "sharepoint",
    "download",
    "client",
    "nodejs",
    "CLI"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koltyakov/sp-download.git"
  },
  "author": "Andrew Koltyakov <andrew.koltyakov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koltyakov/sp-download/issues"
  },
  "homepage": "https://github.com/koltyakov/sp-download#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "commander": "^3.0.2",
    "node-sp-auth-config": "^2.8.0",
    "request": "^2.88.0",
    "sp-request": "^2.1.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/commander": "^2.12.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.11",
    "@types/request": "^2.48.3",
    "chai": "^4.2.0",
    "cpass": "^2.2.0",
    "dotenv": "^8.1.0",
    "mkdirp": "^0.5.1",
    "mocha": "^6.2.1",
    "rimraf": "^3.0.0",
    "spsave": "^3.1.5",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.3"
  }
}
